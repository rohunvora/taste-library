{
  "name": "arena-refs",
  "version": "2.0.0",
  "description": "Reference matching and block classification for Are.na",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "organize": "tsc && node dist/index.js",
    "organize:apply": "tsc && node dist/index.js --apply",
    "classify": "tsc && node dist/manual-classifier.js",
    "taste-profile": "node dist/taste-profile.js",
    "anti-patterns": "node dist/anti-patterns.js",
    "index-blocks": "node dist/index-blocks.js",
    "extract-styles": "node dist/extract-styles.js",
    "ab-test": "node dist/ab-test.js"
  },
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "are.na": "^0.1.5",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/arena-refs"
  },
  "keywords": [
    "arena",
    "are.na",
    "reference",
    "design",
    "classification"
  ],
  "license": "MIT"
}
